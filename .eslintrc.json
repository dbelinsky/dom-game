{
  "plugins": ["prettier"],
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "env": {
    "es6": true,
    "browser": true,
    "jest/globals": false
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "prettier/prettier": [
      "error", 
      { "singleQuote": true }
    ]
  }
}